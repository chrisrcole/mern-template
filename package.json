{
  "name": "apollo-fetch",
  "version": "0.1.0",
  "description": "Web application to shorten links",
  "repository": {
    "type": "git",
    "url": "https://github.com/cccole3/apollo-fetch"
  },
  "author": "Chris Cole",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "dev-server": "nodemon server/server.ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"Server,Client\" -c \"green.bold,cyan.bold\" \"npm run dev-server\" \"cd client && npm run start\"",
    "serve": "node dist/server.js",
    "start": "npm run serve"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.10.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/connect-mongo": "^3.1.3",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/mongoose": "^5.7.36",
    "@types/winston": "^2.4.4",
    "concurrently": "^5.3.0",
    "eslint-config-prettier": "^6.11.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
